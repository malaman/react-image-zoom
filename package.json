{
  "name": "react-image-zoom",
  "version": "1.3.1",
  "description": "react based image zoomer on mouse hover",
  "homepage": "https://github.com/malaman/react-image-zoom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malaman/react-image-zoom.git"
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "js-image-zoom": "^0.7.0",
